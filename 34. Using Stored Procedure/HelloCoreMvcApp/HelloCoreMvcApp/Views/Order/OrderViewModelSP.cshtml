@using Models.ViewModel
@model ICollection<OrderViewModelSP>
@{
    ViewData["Title"] = "OrderViewModelSP";
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
}

<h1>Order View Model Store Procedure</h1>
<hr />

<div class="row">
    <div class="col-md-12">
        <table class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>Customer Name</th>
                    <th>Order No</th>
                    <th>Order Date</th>
                    <th>Total Amount</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
                @foreach (OrderViewModelSP item in Model)
                {
                    <tr>
                        <td>@item.CustomerName</td>
                        <td>@item.OrderNo</td>
                        <td>@item.OrderDate</td>
                        <td>@item.TotalAmount</td>
                        <td>
                            
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

